# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_QUEUE=bottleneck-queue
# RABBITMQ_EXCHANGE=bottleneck-exchange
# RABBITMQ_ROUTING_KEY=bottleneck-routing-key

# Redis Configuration (for Bottleneck)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Bottleneck Configuration
MAX_CONCURRENT=5
MIN_TIME=200
RESERVOIR=100
RESERVOIR_REFRESH_AMOUNT=100
RESERVOIR_REFRESH_INTERVAL=60000

# Temporal Configuration
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=bottleneck-task-queue

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info
